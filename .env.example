# Application Settings
NODE_ENV=production
PORT=3000
LOG_LEVEL=info

# Azure Credentials
AZURE_TENANT_ID=your-tenant-id
AZURE_CLIENT_ID=your-client-id
AZURE_CLIENT_SECRET=your-client-secret
AZURE_SUBSCRIPTION_ID=your-subscription-id

# Azure Monitor Settings
AZURE_LOG_ANALYTICS_WORKSPACE_ID=your-workspace-id
AZURE_APP_SERVICE_RESOURCE_ID=/subscriptions/your-subscription-id/resourceGroups/your-resource-group/providers/Microsoft.Web/sites/your-app-service
AZURE_VM_RESOURCE_ID=/subscriptions/your-subscription-id/resourceGroups/your-resource-group/providers/Microsoft.Compute/virtualMachines/your-vm

# Prometheus Settings
PROMETHEUS_URL=http://prometheus:9090

# Grafana Settings
GRAFANA_URL=http://grafana:3000
GRAFANA_ADMIN_USER=admin
GRAFANA_ADMIN_PASSWORD=admin

# Ollama Settings
OLLAMA_BASE_URL=http://ollama:11434
OLLAMA_MODEL_NAME=llama2 